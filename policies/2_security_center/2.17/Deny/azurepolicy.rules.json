{
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.Security/securityContacts"
            },
            {
                "not": {
                    "field": "Microsoft.Security/securityContacts/phone",
                    "equals": "[parameters('phone')]"
                }
            }
        ]
    },
    "then": {
        "effect": "deny"
    }
}